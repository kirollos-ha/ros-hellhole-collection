<?xml version="1.0"?>
<package format="2">
  <name>driverless_msgs</name>
  <version>0.0.1</version>
  <description>This package contains the messages used by our nodes.</description>
  <maintainer email="driverless.division@firenzerace.it">FRT Driverless Division</maintainer>

  <license>WTFPL</license>
  <buildtool_depend>catkin</buildtool_depend> <!-- This dependency is mandatory for all packages -->
  <build_depend>message_generation</build_depend>
  <build_depend>message_runtime</build_depend>
  <exec_depend>message_runtime</exec_depend>
  <build_export_depend>message_runtime</build_export_depend>
  <depend>std_msgs</depend>
  <depend>visualization_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>geometry_msgs</depend>
  <export></export>
</package>
